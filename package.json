{
  "name": "drizzle-orm-poc",
  "version": "1.0.1",
  "description": "drizzle typescript ORM Proof Of Concept + Cypress testing",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/app.ts",
    "dev:debug": "ts-node-dev --inspect=localhost:5001 src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "db:migration:deploy": "drizzle-kit migrate",
    "db:seeders:deploy": "ts-node-dev --respawn src/db/seeders/seed.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "author": "spinningideas",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "cypress-plugin-api": "^2.11.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.2",
    "express": "^4.21.2",
    "mochawesome": "^7.1.3",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/validator": "^13.12.2",
    "cypress": "^13.17.0",
    "drizzle-kit": "^0.30.1",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
